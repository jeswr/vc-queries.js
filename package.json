{
  "name": "@jeswr/vc-queries",
  "version": "0.0.0-development",
  "description": "A set of queries for the VCs generated by @jeswr/vc-cli",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "queries"
  ],
  "scripts": {
    "semantic-release": "semantic-release",
    "generate": "vc-cli generate --distribute --collect && cp generated/collected.ttl ./collected.ttl && rm -rf generated",
    "query:can-drive": "comunica-sparql-file ./collected.ttl -f ./queries/can-drive.rq",
    "query:employment-status": "comunica-sparql-file ./collected.ttl -f ./queries/employment-status.rq",
    "query:all": "npm run query:can-drive && npm run query:employment-status",
    "test": "npm run generate && npm run query:all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeswr/vc-queries.js"
  },
  "keywords": [],
  "author": "Jesse Wright <https://github.com/jeswr/>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/vc-queries.js/issues"
  },
  "homepage": "https://github.com/jeswr/vc-queries.js/README.md",
  "devDependencies": {
    "@comunica/query-sparql-file": "^4.1.0",
    "@jeswr/vc-cli": "^1.10.1",
    "pre-commit": "^1.2.2",
    "semantic-release": "^24.0.0"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
